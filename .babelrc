{
  "presets": ["react", "es2015", "stage-2"],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": [
        [
          "react-css-modules",
          {
            "generateScopedName": "[name]__[local]__[hash:base64:5]"
          }
        ]
      ],
    },
    "production": {
      "plugins": [
        [
          "react-css-modules",
          {
            "generateScopedName": "[hash:base64:5]"
          }
        ]
      ],
    },
  },
  "comments": false
}
